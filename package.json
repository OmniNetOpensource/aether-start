    "lxgw-wenkai-webfont": "^1.7.0",
    "nes.css": "^2.3.0",
    "openai": "^5.23.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.9.2",
    "streamdown": "^2.3.0",
    "tailwind-merge": "^3.4.0",
    "vite-tsconfig-paths": "^6.0.2",
    "zod": "^3.25.76",
